#!/usr/bin/python3
import random
number = random.randint(-10000, 10000)
last_digit = int(number % 10)
print(f"Last digit of {number:d} is and is less than 6 and not 0", end=" ")
if (last_digit > 5):
    print("and is greater than 5")
elif (last_digit == 0):
    print("and is 0")
else:
    print("and is less than 6 and not 0")
